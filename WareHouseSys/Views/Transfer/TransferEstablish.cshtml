
@{
    ViewBag.Title = "登打移撥單";
}

<style>
    .tdstyle {
        font-family: 標楷體;
        font-size: large;
        vertical-align: middle;
        border-style: solid;
        border-width: 0.5px;
        font-weight: 800;
        text-align: center;
        width: 15%;
        background-color: #D0CECF;
    }

    body {
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    }

    #TransferEstablishGrid .k-grid-content, .k-grid tr td {
        overflow: visible;
    }

    #TransferEstablishGrid .k-grid-header {
        padding-right: 0 !important;
    }
</style>

<div class="row">
    <section class="content-header">
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> 移撥作業</a></li>
            <li class="active">登打移撥單</li>
        </ol>
    </section>
</div>

<button id="TransferEstablishAdd" class="btn btn-danger">新增移撥單</button>

@*Grid*@
<div id="TransferEstablishGrid" data-url="@Url.Action("TransferSearch", "Transfer")"></div>

@*Dialog*@
<div id="TransferEstablishAddDialog" data-url="@Url.Action("TransferEstablishAdd", "Transfer")" ></div>

@section scripts{
    <script src="~/PageJs/TransferEstablishSearch.js?ver=@DateTime.Now.ToString("yyyyMMddmmss")"></script>
    <script src="~/PageJs/TransferEstablishAdd.js?ver=@DateTime.Now.ToString("yyyyMMddmmss")"></script>
}